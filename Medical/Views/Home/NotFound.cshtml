@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Medicals - 404 Not Found</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/errors.css" rel="stylesheet" />
</head>
<body>
    <!-- Error Page -->
    <div class="error">
        <div class="container-fluid">
            <div class="d-flex flex-column justify-content-center text-center w-100 vh-100">
                <div class="container-error-404 d-flex justify-content-center">
                    <div class="position-relative">
                        <div class="clip"><div class="shadow"><span class="digit thirdDigit" data-value="4"></span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit secondDigit" data-value="0"></span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit firstDigit" data-value="4"></span></div></div>
                        <div class="msg">OH!<span class="triangle"></span></div>
                    </div>
                </div>
                <h2 class="h1 d-flex justify-content-center align-items-center gap-2">Sorry! Page not found..! <button class="btn btn-outline-primary fw-bolder fs-5 border-0" id="go-back">Go Back</button></h2>
            </div>
        </div>
    </div>
    <!-- Error Page -->
    <script src="~/js/errors.js"></script>
</body>
</html>
